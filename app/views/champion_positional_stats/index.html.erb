<p id="notice"><%= notice %></p>

<h1>Champion Positional Stats</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @champion_positional_stats.each do |champion_positional_stat| %>
      <tr>
        <td><%= link_to 'Show', champion_positional_stat %></td>
        <td><%= link_to 'Edit', edit_champion_positional_stat_path(champion_positional_stat) %></td>
        <td><%= link_to 'Destroy', champion_positional_stat, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Champion Positional Stat', new_champion_positional_stat_path %>
