<h1> Listing Champions </h1>
<br>                                                                                                                                                                                                         <%= link_to 'Back To Welcome Page', controller: 'welcome_page' %>
<br> 
<table>
	<tr>
		<td> Id </td>
		<td> Name </td>
		<td> Title </td>
	</tr>

	<% @champions.each do |champ| %>
		<tr>
			<td><%= champ.id %></td>
			<td><%= champ.name %></td>
			<td><%= champ.title %></td>
		</tr>
	<% end %>
</table>
